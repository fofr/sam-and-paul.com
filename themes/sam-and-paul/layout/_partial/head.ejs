  // Prefetch the next page of posts
  nextPage = nextPagePath(page);
  if (nextPage) {
%><link rel="prefetch" href="<%= nextPage %>" />
<link rel="prefetch" href="<%= nextPageBackground(page) %>" /><% } %>
