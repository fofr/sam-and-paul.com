<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<%
var title = page.title;

if (page.pages) {
  title = title + ' ('+ page.page + '/' + page.pages +')'
}

if (page.subtitle) {
  title = title + ': ' + page.subtitle
}

if (is_archive()){
  if (is_month()){
    var d = moment([page.year, page.month - 1, 1]);
    title = d.format('MMMM YYYY');
  } else if (is_year()){
    title = page.year;
  }
} else if (is_category()){
  title = page.category;
  title = title.charAt(0).toUpperCase() + title.slice(1);
} else if (is_tag()){
  title = page.tag;
  title = title.charAt(0).toUpperCase() + title.slice(1);
}
%>
<title><% if (title){ %><%= title %> â€” <% } %><%= config.title %></title>

<% if (page.metaDescription) { %>
<meta name="description" content="<%= page.metaDescription %>" />
<% } %>
<!--[if lt IE 9]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
<link rel="stylesheet" href="/css/master.css?<%= config.site_build %>" />
<link rel="alternate" href="<%- theme.rss %>" title="<%= config.title %>" type="application/atom+xml">
<link rel="icon" type="image/png" href="/favicon.png" />
<script src="http://use.typekit.com/fdb0guo.js"></script>
<script>try{Typekit.load();}catch(e){}</script>
